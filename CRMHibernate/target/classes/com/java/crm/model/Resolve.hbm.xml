<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.java.crm.model.Resolve" table="Resolve">
    <id name="ResolveId" column="ResolveID" type="string">
      <generator class="assigned"/>
    </id>
    <property name="ComplaintID" column="ComplaintID"/>
    <property name="ComplaintDate" column="ComplaintDate"/>
    <property name="ResolveDate" column="ResolveDate"/>
    <property name="ResolvedBy" column="ResolvedBy"/>
    <property name="Comments" column="Comments"/>
  </class>
</hibernate-mapping>
